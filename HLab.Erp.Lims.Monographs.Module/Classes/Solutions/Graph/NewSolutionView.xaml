<UserControl x:Class="HLab.Erp.Lims.Monographs.Module.Classes.Solutions.Graph.NewSolutionView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:graph="clr-namespace:HLab.Erp.Lims.Monographs.Module.Classes.Monographs.Graph"
             xmlns:graph1="clr-namespace:HLab.Erp.Lims.Monographs.Module.Classes.Monographs.Graph"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <graph1:BlockGraphView>
    <StackPanel>
            <TextBox Text="{Binding NewSolutionName}" HorizontalAlignment="Center" FontSize="15" FontWeight="ExtraBlack"/>
                <Button HorizontalAlignment="Right" Command="{Binding AddCommand}">Créer</Button>
    </StackPanel>
    </graph1:BlockGraphView>
</UserControl>
