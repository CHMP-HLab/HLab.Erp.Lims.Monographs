<UserControl x:Class="HLab.Erp.Lims.Monographs.Module.Classes.Monographs.Graph.Anchor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Border x:Name="Border"
        Background="White" BorderThickness="2" BorderBrush="Black"
        MouseEnter="UIElement_OnMouseEnter"
        MouseLeave="Border_OnMouseLeave"
        MouseLeftButtonDown ="Border_OnMouseLeftButtonDown"
        MouseLeftButtonUp="Border_OnMouseLeftButtonUp"
            MouseMove="Border_OnMouseMove"
            
            />
    
</UserControl>
