<UserControl x:Class="HLab.Erp.Lims.Monographs.Module.Classes.Monographs.MonographsTreeView.Views.InnContentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:monographs="clr-namespace:HLab.Erp.Lims.Monographs.Module.Classes.Monographs.MonographsTreeView;assembly=HLab.Erp.Lims.Monographs.Module"
             x:Name="_this"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Background="Black"
             IsHitTestVisible="False"
             Margin="0"
             Padding="0"
             d:DataContext="{d:DesignInstance Type=monographs:InnTreeElementViewModel, IsDesignTimeCreatable=True}"
             >
    <Label 
        Foreground="AliceBlue" 
        Content="{Binding Model.Caption}" 
        FontWeight="Bold" 
        FontSize="16"
        >
    </Label>
</UserControl>
