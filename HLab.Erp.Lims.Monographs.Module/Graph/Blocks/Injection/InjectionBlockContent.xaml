<UserControl x:Class="HLab.Erp.Lims.Monographs.Module.Graph.Blocks.Injection.InjectionBlockContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:injection="clr-namespace:HLab.Erp.Lims.Monographs.Module.Graph.Blocks.Injection"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=injection:InjectionBlock, IsDesignTimeCreatable=True}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
            <ContentControl Content="{Binding Gradient}"/>
    </Grid>
</UserControl>
